<% if (theme.mathjax.enable) { %>
  <%- css('prettify/skins/'+ theme.prettify.skin) %>
  <%- js('prettify/prettify.js') %>
  <script type="text/javascript">
    $(document).ready(function() {
        $('pre').addClass('prettyprint linenums').attr('style', 'overflow:auto;');
        prettyPrint();
    });
  </script>
<% } %>